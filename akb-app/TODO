function() {
  switch (t) {
    case "info":
      p.NotificationManager.info("Info message");
      break;
    case "success":
      p.NotificationManager.success("Success message", "Title here");
      break;
    case "warning":
      p.NotificationManager.warning("Warning message", "Close after 3000ms", 3e3);
      break;
    case "error":
      p.NotificationManager.error("Error message", "Click me!", 5e3, function() {
        alert("callback")
      })
  }
}